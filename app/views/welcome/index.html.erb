<p>Welcome to Venturebelt.com</p>

<p><%= link_to 'Sign Up', new_user_path %></p>
<p><%= link_to 'Log In', new_user_path %></p>

<p>Start all gear</p>
<% if @gear %>
  <% @gear.each do |x| %>
  <ol>
    <li><%= x.category %></li>
    <li><%= x.brand %></li>
    <li><%= x.model %></li>
    <li><%= x.year %></li>
    <li><%= x.name %></li>
  </ol>
  <% end %>
<% end %>
<p>End all gear</p>